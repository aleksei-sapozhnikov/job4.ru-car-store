<hibernate-mapping xmlns="http://www.hibernate.org/xsd/hibernate-mapping">
    <class name="carstore.model.Car" table="car">
        <id name="id" column="car_id">
            <generator class="identity"/>
        </id>
        <property name="price" column="price"/>
        <many-to-one name="mark" column="mark_id" class="carstore.model.Mark"/>
        <many-to-one name="body" column="body_id" class="carstore.model.Body"/>
        <many-to-one name="age" column="age_id" class="carstore.model.Age"/>
        <many-to-one name="engine" column="engine_id" class="carstore.model.Engine"/>
        <many-to-one name="chassis" column="chassis_id" class="carstore.model.Chassis"/>
    </class>
</hibernate-mapping>